		      	<div class="col-sm-6">
		      		<div class="footer-inner">
					 	 <h5 class="toggled title">{{ timmings }}</h5>
						 {% if store_timings %}
						<ul class="list-unstyled">
						<li><label style="margin-right: 10px;"><h4>{{ text_day }}</h4></label>&nbsp; | &nbsp; 
						<label style="margin-right: 10px;"><h4>{{ text_opening }}</h4></label>&nbsp; | &nbsp; 
						<label><h4>{{ text_close }}</h4></label></li>	
						   {% for timings in store_timings %}
						   {% if lang == 'ar' %}
						  {% if timings.day_name == 'Saturday' %}
						  {% set newday = 'السبت' %}
						  {% endif %} 
						   {% if timings.day_name == 'Sunday' %}
						  {% set newday = 'الاحد' %}
						  {% endif %} 
						   {% if timings.day_name == 'Monday' %}
						  {% set newday = 'الاثنين' %}
						  {% endif %} 
						   {% if timings.day_name == 'Tuesday' %}
						  {% set newday = 'الثلاثاء' %}
						  {% endif %} 
						   {% if timings.day_name == 'Wednesday' %}
						  {% set newday = 'الاربعاء' %}
						  {% endif %} 
						   {% if timings.day_name == 'Thursday' %}
						  {% set newday = 'الخميس' %}
						  {% endif %} 
						   {% if timings.day_name == 'Friday' %}
						  {% set newday = 'الجمعه' %}
						  {% endif %} 
					
						  {% endif %} 
					
						   {% if timings.day_name ==  "now"|date('l') %}
							<li style="color:red;font-weight:bold;"><label style="margin-right: 10px;">{% if lang == 'ar' %} {{ newday }} {% else %} {{ timings.day_name }}  {% endif %}</label>&nbsp; | &nbsp; 
							<label style="margin-right: 10px;">{{ timings.open_time|date('h:i A') }}</label>&nbsp; | &nbsp; 
							<label>{{ timings.close_time|date('h:i A') }}</label></li>
							 {% else %}
							<li><label style="margin-right: 10px;">  {% if lang == 'ar' %} {{ newday }} {% else %} {{ timings.day_name }}  {% endif %} </label>&nbsp; | &nbsp; 
							<label style="margin-right: 10px;">{{ timings.open_time|date('h:i A') }}</label>&nbsp; | &nbsp; 
							<label>{{ timings.close_time|date('h:i A') }}</label></li>
							
							 {% endif %} 
						   {% endfor %}
					  </ul>
						{% endif %}  
