								{% endif %}
								{% if timings.day_name == 'Wednesday' %}
								{% set newday = 'الاربعاء' %}
								{% endif %}
								{% if timings.day_name == 'Thursday' %}
								{% set newday = 'الخميس' %}
								{% endif %}
