	<script>
		$(function () {

			// plus and minus buttons
			function incrementValue(e) {
				e.preventDefault();
				var fieldName = $(e.target).data('field');
				var parent = $(e.target).closest('div');
				var currentVal = parseInt(parent.find('input[name=' + fieldName + ']').val(), 10);
				if (!isNaN(currentVal)) {
					parent.find('input[name=' + fieldName + ']').val(currentVal + 1);
				} else {
					parent.find('input[name=' + fieldName + ']').val(0);
				}
			}

			function decrementValue(e) {
				e.preventDefault();
				var fieldName = $(e.target).data('field');
				var parent = $(e.target).closest('div');
				var currentVal = parseInt(parent.find('input[name=' + fieldName + ']').val(), 10);

				if (!isNaN(currentVal) && currentVal > 0) {
					var crval = currentVal - 1;
					parent.find('input[name=' + fieldName + ']').val(crval);
					if (crval == 0) {
						parent.find('input[name=' + fieldName + ']').val(1);
						parent.parent().find('.__add_order').removeClass('hidden');
						parent.addClass('hidden');
					}
				} else {
					parent.find('input[name=' + fieldName + ']').val(0);
				}
			}

			$('.input-group').on('click', '.button-plus', function (e) {
				incrementValue(e);
			});

			$('.input-group').on('click', '.button-minus', function (e) {
				decrementValue(e);
			});

			// close cart orders
			$(".__close_cart").click(function () {
				$(".__cart_orders").slideToggle("fast");
			});

			// cart items open and close
			$(".__cart_items").click(function () {
				$(".__cart_orders").slideToggle("fast");
			});

			// add order to cart 
			$(".__add_order").click(function () {
				$("#floater_menu").css("bottom", "70px");
				$("#floater_cart").slideDown("fast");
				$(this).addClass('hidden');
				$(this).parent().find('.input-group').removeClass('hidden');
			});

			// menu on scroll
			window.addEventListener("scroll", function (event) {
				var top = this.scrollY;
				var search_height = ($("#search").position().top);

				//console.log(parseInt(top)+'-'+parseInt(search_height));

				if (parseInt(top) >= parseInt(search_height)) {
					$(".__scroll_hidden").addClass('hidden');
					$(".__scroll_show").removeClass('hidden');
					$(".__account_scroll").removeClass('col-xs-4').removeClass("text-left");
					$(".__account_scroll").addClass('col-xs-2');
					$(".__header_change").addClass("__header");
				}
				else {
					$(".__scroll_hidden").removeClass('hidden');
					$(".__scroll_show").addClass('hidden');
					$(".__account_scroll").addClass('col-xs-4').addClass("text-right");
					$(".__account_scroll").removeClass('col-xs-2');
					$(".__header_change").removeClass("__header");
				}
			}, false);


		})
	</script>
