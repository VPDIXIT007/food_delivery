	<div class="container">
		<div class="header-top">
			<div class="contacts hidden-xs">
				<i class="fa fa-phone"></i>
				<div class="contact-text">
					<div>{{ text_contact }}:</div>
					<a href="{{ contact }}">{{ telephone }}</a>

				</div>
			</div>
			<!-- start logo -->
			<div id="logo">
				{% if logo %}
				<a href="{{ home }}"><img src="{{ logo }}" title="{{ name }}" alt="{{ name }}" class="img-responsive" /></a>
				{% else %}
				<h1><a href="{{ home }}">{{ name }}</a></h1>
				{% endif %}
			</div>
			<div class="header-right header-links">
				<!-- start cart -->
				<div class="header_cart">{{ cart }}</div>


			</div>
		</div>
	</div>
	<div class="header-bottom">
		<div class="container">
			<div class="header-bottom-inner">
				<!-- start menu -->
				{{ menu }}
				<!-- start search -->
				<div class="btn_search">
					<div class="header-search">
						{{ search }}
					</div>
				</div>

			</div>
		</div>
	</div>
	</header>


	<div class='floaterdiv hidden'>
		<div id="mySideshare" class="icon-bar sharebar hidden">
			<div id='sharebutton' class="facebook">

				{% if facebook_link %}
				<a href="https://{{ facebook_link }}" class="facebook" target="blank"><i class="fa fa-facebook"></i></a>
				{% endif %}
				{% if wesbsite_link %}
				<a href="https://{{ wesbsite_link }}" class="instagram" target="blank"><i class="fa fa-globe"
						aria-hidden="true"></i></a>
				{% endif %}
				{% if instagram_link %}
				<a href="https://{{ instagram_link }}" class="instagram" target="blank"><i class="fa fa-instagram"
						aria-hidden="true"></i></a>
				{% endif %}
				{% if whatsapp_link %}
				<a href="{{ whatsapp_link }}" class="whatsapp" aria-hidden="true" target="_blank"><i
						class="fa fa-whatsapp"></i></a>

				{% endif %}
				| <i class="fa fa-link "></i>
			</div>
		</div>

		<div id="mySidenav" class='icon-bar mySideshare sharebutton'>
			<a href="#" class="linkedin" data-toggle="modal" data-target="#myModal"><i class="">{{ order_tracking2 }}</i></a>
		</div>
	</div>


	<!-- Modal -->
	<div class="modal fade" id="myModal" role="dialog">
		<div class="modal-dialog ">

			<!-- Modal content-->
			<div class="modal-content">
				<div class="modal-header">
					<button type="button" class="close" data-dismiss="modal">&times;</button>
					<h4 class="modal-title">{{ tracking_order }}</h4>
				</div>
				<div class="modal-body">
					<div class="form-group">
						<label for="exampleInputEmail1">{{ order_no }}</label>
						<input type="text" id="order_no" aria-describedby="emailHelp" name="orderno"
							value="{% if tracking_order_no %} {{tracking_order_no}} {% endif %}">
						<button onclick="trackorder();">{{ track }}</button>
					</div>
					<div id="result"></div>
				</div>
				<div class="modal-footer">
					<button type="button" class="btn btn-default" data-dismiss="modal">{{ close }}</button>
				</div>
			</div>

		</div>
	</div>

	<script>

		{% if tracking_order_no %}
		var trackno = {{ tracking_order_no }};
		if (trackno) {
			$(".linkedin").on('click', function () {
				trackorder();
			});

		}
		{% endif %}
		function trackorder() {
			let orderno = $('#order_no').val();

			$.ajax({
				url: 'index.php?route=order_tracking/order_tracking/track',
				type: 'post',
				dateType: 'json',
				data: { order_no: orderno, sellerstore: '{{ sellerstore }}' },
				success: function (json) {
					if (json.success) {
						console.log(json['status_name']['name']);
						$('#result').html('<div class="orderstatus">Order Status : ' + json['status_name']['name'] + '</div>');
					} else {
						$('#result').html('<div class="orderstatus">' + json['message'] + '</div>');
					}
				}
			});
		};
	</script>
	<!--End Modal -->

