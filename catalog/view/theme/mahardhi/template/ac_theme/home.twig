		</div>
	</div>


	<!-- Modal -->
	<div class="modal fade" id="myModal" role="dialog">
		<div class="modal-dialog ">

			<!-- Modal content-->
			<div class="modal-content">
				<div class="modal-header">
					<button type="button" class="close" data-dismiss="modal">&times;</button>
					<h4 class="modal-title">{{ tracking_order }}</h4>
				</div>
				<div class="modal-body">
					<div class="form-group">
						<label for="exampleInputEmail1">{{ order_no }}</label>
						<input type="text" id="order_no" aria-describedby="emailHelp" name="orderno"
							value="{% if tracking_order_no %} {{tracking_order_no}} {% endif %}">
						<button onclick="trackorder();">{{ track }}</button>
					</div>
					<div id="result"></div>
				</div>
				<div class="modal-footer">
					<button type="button" class="btn btn-default" data-dismiss="modal">{{ close }}</button>
				</div>
			</div>

		</div>
	</div>

	<script>

		{% if tracking_order_no %}
		var trackno = {{ tracking_order_no }};
		if (trackno) {
			$(".linkedin").on('click', function () {
				trackorder();
			});

		}
		{% endif %}
		function trackorder() {
			let orderno = $('#order_no').val();

			$.ajax({
				url: 'index.php?route=order_tracking/order_tracking/track',
				type: 'post',
				dateType: 'json',
				data: { order_no: orderno, sellerstore: '{{ sellerstore }}' },
				success: function (json) {
					if (json.success) {
						console.log(json['status_name']['name']);
						$('#result').html('<div class="orderstatus">Order Status : ' + json['status_name']['name'] + '</div>');
					} else {
						$('#result').html('<div class="orderstatus">' + json['message'] + '</div>');
					}
				}
			});
		};
	</script>
	<!--End Modal -->

