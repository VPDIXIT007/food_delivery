										<p class="price"> 
											{% if not product.special %}
												{{ product.price }}
											{% else %}
												<span class="price-new">{{ product.special }}</span><span class="price-old">{{ product.price }}</span>
											{% endif %}
										</p>
									{% endif %}									
								</div>
								<div class="samerow">
								 <div class="limitwidth pro-quantity">									
		                            <div class="minus-plus">
		                            	<button id="q_down{{ product.product_id }}" class="q_down minus"><i class="fa fa-minus"></i></button>	                            	
		                            	<input type="text" name="quantity" value="1" size="2" id="quantity_wanted{{ product.product_id }}" class="hmm12 form-control">
		                            	<button id="q_up{{ product.product_id }}" class="q_up plus"><i class="fa fa-plus"></i></button>
											
								   </div>    
								    <script type="text/javascript" async="">$("#q_up{{ product.product_id }}").on("click", function() {var $button =$(this);var oldValue = $('#quantity_wanted{{ product.product_id }}').val();var newVal = parseFloat(oldValue) + 1;$('#quantity_wanted{{ product.product_id }}').val(newVal);});$("#q_down{{ product.product_id }}").on("click", function() {var $button = $(this);var oldValue = $('#quantity_wanted{{ product.product_id }}').val();if (oldValue > 1) {var newVal = parseFloat(oldValue) - 1;} else {newVal = 1;}$('#quantity_wanted{{ product.product_id }}').val(newVal);});</script>
     			        
	                            </div>
								<div class="cart_main">
								
									<button type="button" class="addcart" title="{{ button_cart }}" onclick="cartaddquantity('{{ product.product_id }}');">{{ button_cart }}</button>
								</div>
								</div>
							</div>
						</div>
					</div>
					{% endfor %}
